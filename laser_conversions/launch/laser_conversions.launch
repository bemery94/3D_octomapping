<?xml version="1.0"?>

<launch>
    <!--Converts PointCloud message to PointCloud2. The PointCloud2 msg is used for the octomap-->
    <node type="conv_cloud_to_cloud2" name="conv_cloud_to_cloud2"
        pkg="laser_conversions">
        <remap from="cloud_in" to="laser_to_cloud_out" />
        <remap from="cloud2_out" to="assembled_cloud2_out" />
    </node>

<node type="conv_laser_to_cloud" name="conv_laser_to_cloud" pkg="laser_assembler_conversions">
    <remap from="laser_scan_in" to="/scan_lsm"/>
</node>
</launch>
